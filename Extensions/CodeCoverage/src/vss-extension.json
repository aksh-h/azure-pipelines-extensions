{
  "manifestVersion": 1,
  "id": "canarys-services-codecoverage",
  "name": "Verify Code Coverage",
  "publisher": "canarysautomationspvtltd",
  "version": "1.8",
  "public": true,
  "description": "Code Coverage with Azure Pipelines",
  "categories": [
    "Azure Pipelines"
  ],
  "Tags": [
    "Release",
    "Release Gates",
    "DevOps",
    "Pipelines"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services.Cloud"
    }
  ],
  "demands": [],
  "icons": {
    "default": "images/codecoverage1.png"
  },
  "files": [
    {
      "path": "images/add-code-coverage.png",
      "addressable": true
    },
    {
      "path": "images/code-coverage-failed.png",
      "addressable": true
    },
    {
      "path": "images/code-coverage-passed.png",
      "addressable": true
    },
    {
      "path": "images/configure-code-coverage.png",
      "addressable": true
    },
    {
      "path":"images/codecoverage1.png",
      "addressable": true
    },
    {
      "path": "Tasks/CodeCoverage"
    }    
  ],
  "content": {
    "details": {
      "path": "readme.md"
    }
  },
  "contributions": [
    {
      "id": "serviceNowchangerequests-query-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/CodeCoverage"
      }
    }
  ]
}
