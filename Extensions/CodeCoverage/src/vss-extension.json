{
  "manifestVersion": 1,
  "id": "vss-services-codecoverage",
  "name": "Verify Code Coverage",
  "publisher": "akshayh",
  "version": "0.0.4",
  "public": false,
  "description": "Code Coverage with Azure Pipelines",
  "categories": [
    "Azure Pipelines"
  ],
  "Tags": [
    "Release",
    "Release Gates",
    "DevOps",
    "Pipelines",
    "Code Coverage"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services.Cloud"
    }
  ],
  "demands": [],
  "icons": {
    "default": "images/codecoverage.png"
  },
  "files": [
    {
      "path": "Tasks/CodeCoverage"
    }    
  ],
  "content": {
    "details": {
      "path": "readme.md"
    }
  },
  "contributions": [
    {
      "id": "codecoverage-validate-gate",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/CodeCoverage"
      }
    }
  ]
}
